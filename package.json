{
  "name": "tictactoe",
  "version": "0.2.0",
  "author": "redpelicans",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --color",
    "test": "NODE_ENV=testing mocha --require babel-core/register --reporter spec \"./**/__test__/*.js\" ",
    "build:prod": "NODE_ENV=production webpack -p --config webpack.config.js --progress",
    "build:dev": "NODE_ENV=development webpack-dev-server --progress --config webpack.config.js",
    "start": "nodemon --exec 'babel-node ./api/index.js --debug'",
    "bundle:stats": "NODE_ENV=production webpack -p --config webpack.config.js --profile --json > stats.json",
    "bundle:analyse": "webpack-bundle-analyzer ./stats.json ./build --report --mode=static",
    "coverage": "NODE_ENV=testing nyc yarn test"
  },
  "dependencies": {
    "antd": "^2.6.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.7.0",
    "express": "^4.14.0",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "ramda": "^0.22.1",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.0.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-socket.io": "^1.3.1",
    "redux-thunk": "^2.1.0",
    "sinon": "^1.17.7",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "styled-components": "^1.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.26",
    "webpack-bundle-analyzer": "^1.5.0",
    "webpack-dev-server": "^2.1.0-beta.10"
  },
  "nyc": {
    "exclude": [
      "build"
    ]
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ]
  }
}
